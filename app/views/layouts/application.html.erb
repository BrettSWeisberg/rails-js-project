<!DOCTYPE html>
<html>
<head>
  <% if session[:manager_id] %>
  <%= link_to "#{current_manager.name}'s Profile ", manager_path(current_manager) %>
  <%= link_to  'Log Out', logout_path, method: :delete %>
  <%end%>
  <%= link_to("Home", '/') unless current_page?('/') %>

  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>

</head>
<body>

<%= yield %>

</body>
</html>
